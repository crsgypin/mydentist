

<div class="search_result">
	<ul>
	<% @events.each do |event| %>
		<li data-id="<%= event.id %>">
			<span><%= event.patient.name %></span>
			<span><%= event.desc_format(3).gsub(" ","<br>").html_safe %></span>
			<span><%= event.service.name %></span>
			<span><%= event.doctor.name %><br><%= event.doctor.title %></span>
		</li>
	<% end %>
	</ul>
</div>

