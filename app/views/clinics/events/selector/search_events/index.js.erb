
$("#date_search_nav .search_box .search_result").remove();
$("#date_search_nav .search_box").append("<%= j render_relative_partial 'index/table' %>");

$("body").on("click.search", function(e){
	if($(e.target).closest(".search_result").length == 0){
		$("#date_search_nav .search_box .search_result").remove();
		$("body").off("click.search");
	}
});
