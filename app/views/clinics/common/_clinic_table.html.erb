
<table id="clinic_table">
	<tr>
		<% cols.each do |col| %>
		<th>
			<%= col[:t] %>
		</th>
		<% end %>
	</tr>
	<% resources.each do |resource|%>
	<tr>
		<% cols.each do |col| %>
		<td <%= "onclick=\"window.location.href= '#{row[:link].call(resource)}'\"".html_safe if !col[:unclickable] %>>
			<%= col[:v].call(resource) %>
		</td>
		<% end %>
	</tr>
	<% end %>
</table>


