
<%= render "clinics/clinic_line/common/line-templates-box", locals: {
	line_templates: @clinic_line_knowledges.map do |knowledge|
		knowledge.line_template
	end,
	edit_path: proc do |line_template|
		clinic_line_knowledge = @clinic_line_knowledges.find{|k| k.line_template == line_template}
		url_for(action: :edit, id: clinic_line_knowledge)
	end,
	new_path: url_for(action: :new)
} %>

