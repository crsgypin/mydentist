
<div id="clinic-line-keywords">
	<%= render_relative_partial "../common/line-templates-box", locals: {
		line_templates: @clinic_line_keywords.map do |keyword|
			keyword.line_template
		end,
		edit_path: proc do |line_template|
			clinic_line_keyword = @clinic_line_keywords.find{|k| k.line_template == line_template}
			url_for(action: :edit, id: clinic_line_keyword)
		end,
		new_path: url_for(action: :new)
	} %>
</div>
